{
  "name": "@kunalnagarco/imdb-scraper",
  "version": "1.0.4",
  "description": "An attempt at the most complete IMDb API",
  "keywords": [
    "imdb",
    "http",
    "api",
    "scrape"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kunalnagarco/imdb-scraper.git"
  },
  "license": "MIT",
  "author": "Kunal Nagar <knlnagar@gmail.com>",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix src/ tests/ --ext .js,.ts,.tsx --max-warnings 0",
    "start": "node dist/index.js",
    "build": "tsc",
    "test": "ts-mocha -p tsconfig.json tests/**/*.spec.ts -t 150000",
    "test:custom": "start-server-and-test 'yarn build && yarn start' http://localhost:3000 'yarn test'",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "lint-staged": {
    "package.json": [
      "sort-package-json"
    ],
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix src/ tests/ --ext '*/**/*.{js,ts,tsx}' --max-warnings 0"
    ]
  },
  "resolutions": {
    "mem": "8.1.1",
    "minimist": "1.2.7",
    "yargs-parser": "20.2.9"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "node-fetch": "^2.6.0",
    "underscore": "1.13.6"
  },
  "devDependencies": {
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.1",
    "@semantic-release/github": "7.2.3",
    "@semantic-release/npm": "7.1.3",
    "@semantic-release/release-notes-generator": "9.0.3",
    "@types/chai": "4.3.3",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.15",
    "@types/mocha": "8.2.3",
    "@types/node-fetch": "2.6.2",
    "@types/underscore": "1.11.4",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "chai": "4.3.6",
    "conventional-changelog-conventionalcommits": "4.6.3",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "6.0.0",
    "lint-staged": "11.2.6",
    "mocha": "9.2.2",
    "prettier": "2.8.0",
    "request": "2.88.2",
    "semantic-release": "19.0.5",
    "sort-package-json": "1.57.0",
    "start-server-and-test": "1.14.0",
    "ts-mocha": "8.0.0",
    "typescript": "4.8.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
